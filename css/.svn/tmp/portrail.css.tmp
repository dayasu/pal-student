*{
	font-family:"Helvetica Neue",Helvetica,STHeiTi,sans-serif;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td{
	margin:0;
	padding:0
};

ol,li{
	list-style:none;
}
a:link,a:hover{text-decoration:none}
a:visited,a:active{text-decoration:none}
.css{-webkit-transform:translate3d(0,0,0);-moz-transform:translate3d(0,0,0);-ms-transform:translate3d(0,0,0);transform:translate3d(0,0,0)}
html{height:100%}body{width:100%}
html{font-size:10px}
@media screen and (min-width:321px) and (max-width:375px){html{font-size:11px}}
@media screen and (min-width:376px) and (max-width:414px){html{font-size:12px}}
@media screen and (min-width:415px) and (max-width:639px){html{font-size:15px}}
@media screen and (min-width:640px) and (max-width:719px){html{font-size:20px}}
@media screen and (min-width:720px) and (max-width:749px){html{font-size:22.5px}}
.clear{
	clear:both;
}
.contanier{
	width:100%;
}
#header{
	margin:auto;
	background:url(../images/wap/back.jpg) no-repeat;
}
.por{
	margin:auto;
	position:relative;
	height:48%;
	width:24%;
}

.circle{
	height:100%;
	width:100%;
	float:left;
	position:absolute;
	top:0;
	left:0;
}
.teacher-por{
	height:100%;
	width:100%;
	border-radius: 100px;
}





#teacher{
	height:100px;
	background: yellow;
}

#subject{
	height:100px;
	background: blue;
}
#self{
	height:100px;
	background: green;
}
#other{
	height:100px;
	background: gray;
}





































































