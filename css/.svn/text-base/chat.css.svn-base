.chat-close{background:none repeat scroll 0 0 #5abed0;bottom:0;overflow:hidden;position:fixed;right:20px;width:300px;height:50px;line-height:50px;z-index:50;padding:0 10px;font-size:20px;color:#fff}.chat-win{background:none repeat scroll 0 0 #eee;bottom:0;overflow:hidden;position:fixed;right:20px;z-index:100;padding-bottom:20px;border-style:solid;border-width:2px;border-color:#5abed0;display:none}.chat-top{background-color:#5abed0;height:40px;line-height:40px;padding:0 10px;font-weight:bold;font-size:18px;color:#fff}.chat-top span,.chat-close span{cursor:pointer}.chat-usr{float:left;height:400px;overflow-x:hidden;overflow-y:auto;padding:5px 10px;border-right:1px solid #ccc}.chat-usr li{height:30px;line-height:30px;cursor:pointer;white-space:nowrap;overflow:hidden;text-overflow:ellipsis}.chat-usr li:hover{background-color:white}.chat-main{float:left;width:420px;height:400px}#msg{height:300px;padding:20px 15px;overflow-x:hidden;overflow-y:auto;margin:20px 0;border-bottom:1px solid #ccc}.chat-cont-recv,.chat-cont-send{border-style:solid;border-width:3px;border-radius:0 15px 15px;border-color:#ddd;color:#000;display:inline-block;float:left;padding:6px 10px;margin:5px 0;word-break:break-all;clear:both}.chat-cont-send{border-color:#5abef0;border-radius:15px 0 15px 15px;float:right}