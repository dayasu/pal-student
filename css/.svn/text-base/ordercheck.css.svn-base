.main-body{width:1000px;margin:auto;background-color:#fff}.check-main{margin:50px 0}.check-main .check-step{border:1px solid #ddd;padding:30px 20px}.order-addr .radio-addr{padding:10px 20px;width:600px;border-radius:12px}.order-addr .selected{background-color:#b3c85e}.check-table{width:100%;max-width:100%;margin:50px 0 50px}.check-table th,.check-table td{padding:8px;line-height:1.5;text-align:center;vertical-align:baseline}.check-table>thead>tr>th{background-color:#afd9ff;border:2px solid #fff}.check-table>tbody>.check-item{border-bottom:1px dashed #ddd}.check-table>tbody>.check-annex{border-top:1px solid #ddd;background-color:#f5f5f5}.check-table>tbody>.check-annex>td{padding:10px 25px;text-align:left}.check-table>tbody>.check-submit>td{padding:15px 0;text-align:right}.check-table .check-submit em{padding:0 3px;color:#c00;font-style:normal;font-weight:bold;font-size:150%}#subBtn{padding:12px 40px;font-size:120%;color:#fff;background-color:#ef494d;border-radius:5px;font-weight:bold;cursor:pointer}