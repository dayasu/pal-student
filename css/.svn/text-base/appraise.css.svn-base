.star {
    display: block;
    height: 100%;
    width: auto;
}
.star span {
    width: 30px;
    height: 30px;
    display: inline-block;
    position: relative;
    top: 8px;
}
.star span {
    background:url(../images/star_bg.png) no-repeat;
    background-size: 300%;
    background-position: right;
}
.to_appraise .star span {
    cursor: pointer;
}
